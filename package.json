{
  "name": "uki",
  "version": "0.8.0",
  "description": "Minimal, d3-based Model-View library",
  "module": "dist/uki.esm.js",
  "jsnext:main": "dist/uki.esm.js",
  "scripts": {
    "example": "bash examples/run.sh",
    "build": "rollup -c",
    "lint": "eslint **/*.js --quiet",
    "dev": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukijs/uki.git"
  },
  "author": "Alex Bigelow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ukijs/uki/issues"
  },
  "homepage": "https://github.com/ukijs/uki#readme",
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "no-extra-semi": 2,
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ]
    },
    "globals": {
      "globalThis": false
    }
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "eslint": "^8.0.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "rollup": "^2.58.0",
    "rollup-plugin-execute": "^1.1.1",
    "serve": "^12.0.1"
  },
  "peerDependencies": {
    "d3": "^7.1.1"
  },
  "optionalDependencies": {
    "less": "^4.1.2"
  }
}
